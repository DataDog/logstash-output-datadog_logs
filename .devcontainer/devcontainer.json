{
    "name": "Logstash Plugin Dev",
    "dockerFile": "Dockerfile",
    "remoteUser": "root",
    "postCreateCommand": ".devcontainer/post-launch.sh",
    "remoteEnv": {
        "PATH": "/usr/share/logstash/vendor/jruby/bin:/usr/share/logstash/jdk/bin:${containerEnv:PATH}"
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "rebornix.ruby",
                "ms-azuretools.vscode-docker"
            ]
        }
    }
}